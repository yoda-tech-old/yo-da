<h2>Companies that have your data</h2>
<table class="table">
  <thead class="thead-light">
    <th scope="col">From Email</th>
    <th scope="col">From Name</th>
    <th scope="col">Received</th>

  </thead>
  <tbody>

    {{#each messages}}
      <tr class="{{#unless this.isRead}}table-primary{{/unless}}">
        <td>{{this}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
